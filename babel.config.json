{
  "plugins": [
    "@babel/plugin-transform-runtime",
    "@babel/plugin-transform-typescript",
    ["module-resolver", {
      "root": ["./src"],
      "alias": {
        "src/mq": "./src/mq",
        "src/chain": "./src/chain",
        "src/router": "./src/router",
        "src/logger": "./src/logger",
        "src/db": "./src/db"
      },
      "extensions": [".js", ".ts"]
    }]
  ],
  "presets": [
    [
      "@babel/preset-env", {
      "targets": {"node": true}
    }],
    "@babel/preset-typescript"
  ],
  "comments": false
}
